trigger1 = Time = 12
value = 2, 2

[State 220, Voice]
type = PlaySnd
trigger1 = Time = 13
;freqmul = 1.1
channel = 1
value = 5, 2

[State 220, throw attempt]
type = HitDef
trigger1 = AnimElem = 1
